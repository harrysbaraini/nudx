[global]
pid={{fpmPidFile}}
error_log={{phpStateDir}}/php-fpm.log
emergency_restart_threshold=10
emergency_restart_interval=1m
process_control_timeout=10s

[web]
pm=dynamic
pm.max_children=5
pm.start_servers=3
pm.min_spare_servers=2
pm.max_spare_servers=4
pm.max_requests=100
listen={{fpmSocketFile}}
