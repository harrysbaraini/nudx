{
  "name": "@nudx/cli",
  "version": "0.2.0",
  "description": "A simple CLI to create development environments",
  "author": "Vanderlei Sbaraini Amancio @harrysbaraini",
  "bin": {
    "nudx": "./bin/run"
  },
  "homepage": "https://github.com/harrysbaraini/nudx",
  "license": "MIT",
  "repository": "harrysbaraini/nudx",
  "files": [
    "/bin",
    "/lib",
    "/files",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@nudx/plugin-git": "^0.1.0",
    "@oclif/core": "^2",
    "@oclif/plugin-help": "^5",
    "@oclif/plugin-plugins": "^3.1.2",
    "handlebars": "^4.7.7",
    "inquirer": "^8",
    "js-yaml": "^4.1.0",
    "listr": "^0.14.3",
    "node-netstat": "^1.8.0",
    "pm2": "^5.3.0",
    "rxjs": "^7.8.1"
  },
  "oclif": {
    "bin": "nudx",
    "dirname": "nudx-cli",
    "commands": "./lib/commands",
    "hooks": {
      "init": "./lib/hooks/init/init"
    },
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins",
      "@nudx/plugin-git"
    ],
    "topicSeparator": " ",
    "topics": {
      "site": {
        "description": "Commands to manage sites"
      }
    }
  },
  "scripts": {
    "compile-templates": "node ./compile-templates.js",
    "build": "yarn compile-templates && shx rm -rf lib && tsc",
    "postpack": "shx rm -f oclif.manifest.json",
    "prepack": "yarn build && oclif manifest",
    "buildpack": "oclif pack tarballs",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md",
    "pre-commit": "yarn run format && yarn run lint-fix && yarn run lint"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "bugs": "https://github.com/harrysbaraini/nudx/issues",
  "keywords": [
    "nudx",
    "development",
    "environment",
    "docker",
    "nix",
    "nodejs",
    "php",
    "laravel",
    "sql"
  ],
  "types": "lib/index.d.ts",
  "exports": {
    ".": "./lib/index.js"
  }
}
